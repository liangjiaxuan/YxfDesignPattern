<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rongwei.yxfdesignpattern">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.observer.ObserverActivity" />
        <activity android:name=".activity.factory.FactoryActivity" />
        <activity android:name=".activity.singleton.SingletonActivity" />
        <activity android:name=".activity.strategy.StrategyActivity" />

        <activity android:name=".activity.TestWebviewActivity" />
        <activity android:name=".activity.TestJsActivity"></activity>
    </application>

</manifest>